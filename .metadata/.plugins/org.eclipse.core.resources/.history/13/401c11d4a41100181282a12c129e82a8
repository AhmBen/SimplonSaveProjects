import java.sql.SQLException;

public class Aliment {

	
	
	private static String energyUnit = "KCal";
	
	private static String refWeitght = "100 g";
	
	private String name	= "";
	private int protein	= 0;
	private int glucid	= 0;
	private int lipid	= 0;
	
	
	public Aliment(String name,int protein,int glucid,int lipid) {
		this.name = name;
		this.protein = protein;
		this.glucid = glucid;
		this.lipid = lipid;
	}
	
	public String toString()
	{
	return "Je suis "+this.name+" Bilan : protéine="+this.protein+" g /"+Aliment.refWeitght+" Glucide = "+this.glucide+" g /"+Aliment.refWeitght+" Lipide "+this.lipid+" g /"+Aliment.refWeitght;
	}
	
	public static void listAliment()
	{
		
	}
	
	
}
